body {
	font: 14px MicrosoftYaHei;
}

/*易涝监测点start*/
.flood_content{
	overflow: hidden;
}
.flood_title{
	height: 38px;
	/*font-family: MicrosoftYaHei;*/
	font-size: 16px;
	font-weight: normal;
	color: #323232;
	line-height: 38px;
	border-bottom: 1px solid #e0e0e0;
	margin-left: 10px;
	padding-left: 10px;
	letter-spacing: 1px;
	position:relative;
}
.flood_box{
	width: 240px;
	height: 323px;
	background-color: #ffffff;
	border:1px solid #e0e0e0;
	margin:10px 10px 20px 10px;
	box-shadow: 0px 1px 4px 0px rgba(133, 160, 179, 0.2);
}
.color_title{
	width: 5px;
	height: 16px;
	background-color: #0095ff;
	position: absolute;
	left: 0;
	top:12px;
}
.flood_small{
	width: 100%;
	height: 39px;
	border-bottom: 1px solid #e0e0e0;
	text-align: center;
	line-height: 39px;
	font-size: 18px;
	color: #323232;
}
.flood_cons{
	width: 200px;
	height: 245px;
	background-color: #ffffff;
	box-shadow: 0px 1px 4px 0px rgba(133, 160, 179, 0.2);
	margin:0 auto;
	margin-top:20px;
}
.flood_cons_title{
	width: 100%;
	height: 40px;
	background-color: #0095ff;
	line-height: 40px;
	position:relative;
}
.flood_cons_title span{
	color: #fff;
	font-size: 14px;
	padding-left: 10px;
}
.flood_cons_title img{
	position: absolute;
	right: 10px;
	top:13px;
}
.flood_cons_detail h3{
	font-size:14px;
	color: #0095ff;
	margin-top:8px;
	padding-left: 8px;
}
.flood_cons_detail h4{
	font-size:14px;
	color: #0095ff;
	padding-left: 8px;
}
.flood_cons_detail input.input_flood_one{
	border:0;
	width: 181px;
	height: 50px;
	text-align: center;
	line-height: 50px;
	font-size: 28px;
	color: #d98100;
	margin:8px 0;
	margin-left: 10px;
}
.flood_cons_detail input.input_flood_two{
	border:0;
	width: 181px;
	height: 50px;
	text-align: center;
	line-height: 50px;
	font-size: 28px;
	color: #b70000;
	margin-left: 10px;
}
input:disabled{
	border: 1px solid #DDD;
	background-color:#FFFFFF;
	color:#ACA899;
}
.flood_time{
	width: 181px;
	height: 30px;
	margin:0 auto;
	line-height: 30px;
	color: #888888;
	font-size: 12px;
	position: relative;
	border-top:1px solid #F0F0F0;
	margin-top:12px;
}
.flood_time b{
	font-weight: normal;
}
.flood_time span{
	position: absolute;
	right: 0;
}
.flood_time1{
	width: 181px;
	height: 30px;
	margin:0 auto;
	line-height: 30px;
	color: #888888;
	font-size: 12px;
	position: relative;
	border-top:1px solid #F0F0F0;
	margin-top:12px;
}
.flood_time1 b{
	font-weight: normal;
}
.flood_time1 span{
	position: absolute;
	right: 0;
}
/*易涝监测点end*/
/*水质监测点start*/
.water-fluid{
	margin-top:-15px;
}
.water_box{
	width: 200px;
	height: 245px;
	background-color: #ffffff;
	box-shadow: 0px 1px 4px 0px 
	rgba(133, 160, 179, 0.2);
	margin:20px 5px;
}
.water_first{
	margin-left: 20px;
}
.water_big_box{
	width: 870px;
	height: auto;
	margin:15px 0 15px 10px;
	border: 1px solid #e0e0e0;
}
.water_title{
	height: 39px;
	/*font-family: MicrosoftYaHei;*/
	font-size: 16px;
	font-weight: normal;
	color: #323232;
	line-height: 39px;
	border-bottom: 1px solid #e0e0e0;
	text-align: center;
	position:relative;
}
.detail_top{
	width: 181px;
	height: 50px;
	line-height: 50px;	
	color:#d98100;
	margin:0 auto;
	margin-top:11px;
	margin-bottom:5px;
}
.detail_bottom{
	width: 181px;
	height: 46px;
	line-height: 50px;
	color:#b70000;
	margin:0 auto;
	padding-top:4px;
}
.cons_left{
	width: 80px;
	height:100%;
	float: left;
	padding-left: 8px;
}
.rangTopOne{
	width: 60px;
	height: 30px;
	border:0;
	font-size: 28px;
	font-weight: normal;
	color:#d98100;
}
.rangTopTwo{
	width: 66px;
	height: 30px;
	border:0;
	font-size: 28px;
	font-weight: normal;
	color:#d98100;
}
.rangTopOne:disabled{
	width: 42px;
	height: 30px;
	border:0;
	font-size: 28px;
	font-weight: normal;
	color:#d98100;
}
.rangTopTwo:disabled{
	width: 56px;
	height: 30px;
	border:0;
	font-size: 28px;
	font-weight: normal;
	color:#d98100;
}
#water_range_box{
	box-shadow: 0px 1px 4px 0px rgba(133, 160, 179, 0.2);
}
.cons_left b{
	font-size: 28px;
	font-weight: normal;
}
.cons_right{
	width: 80px;
	height: 100%;
	float: right;
	position: relative;
}
.cons_right span{
	font-size: 28px;
	font-weight: normal;
}
.monitoringSet{
	width: 200px;
	height: 100px;
}
.rangeBottomOne{
	width: 60px;
	height: 30px;
	border:0;
	font-size: 28px;
	font-weight: normal;
	color:#b70000;
}
.rangeBottomTwo{
	width: 66px;
	height: 30px;
	border:0;
	font-size: 28px;
	font-weight: normal;
	color:#b70000;
}
.rangeBottomOne:disabled{
	width: 42px;
	height: 30px;
	border:0;
	font-size: 28px;
	font-weight: normal;
	color:#b70000;
}
.rangeBottomTwo:disabled{
	width: 56px;
	height: 30px;
	border:0;
	font-size: 28px;
	font-weight: normal;
	color:#b70000;
}
/*水质监测点end*/
.confirmBox{
	width: 180px;
	height: 30px;
	margin:0 auto;
	margin-top:12px;
	border-top:1px solid #F0F0F0;
}
.confirmBtn{
	border:0;
	widows: 26px;
	height: 13px;
	line-height: 13px;
	background: #ffffff;
	margin-top:9px;
	margin-left:77px;
	font-size: 12px;
	color: #0095ff;;
}
.copyright {
	text-align: center;
	width: 100%;
    height: 30px;
    line-height: 30px;
    box-shadow: 0px 1px 0px 0px rgba(224, 224, 224, 0.3);
    /* font-family: MicrosoftYaHei; */
    font-size: 14px;
    color: #888888;
    background: #fff;
}
-webkit-input-placeholder { /* WebKit 核心浏览器 如谷歌Chrome */color:#999;}-moz-placeholder { /* 火狐浏览器 */color:#999;}-ms-input-placeholder { /* IE 10+ */color:#999;}/* www.hi-docs.com/css.html */
@media all and (-ms-high-contrast: none), (-ms-high-contrast: active) {
	body {
		font: 14px Helvetica Neue, Helvetica, PingFang SC, 微软雅黑, Tahoma, Arial, sans-serif;
	}
}
